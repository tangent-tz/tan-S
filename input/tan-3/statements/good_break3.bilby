func void sequence(int startingNumber) {
	print $n;
	
	mut num := startingNumber;
	print num $n;
	while (num != 1) {
		num := num / 2;
		if(num == 8) {
			break;
		}
		print num $n;
	}
	print num $n;
	return;
}

main {
	imm min := 10;
	imm max := 20;
	
	mut count := min;
	while (count < max) {
		call sequence(count);
		count := count + 1;
	}
	print "done." $n;
}
