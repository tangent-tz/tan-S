

func [float] apply([int] a){
	imm result := alloc [float](length a);
	
	mut i := 0;
	while (i < length a) {
		result[i] := pax(a[i]);
		i := i + 1;
	}
	return result;
}

func void pax(int b){
	if (b < 4) {
	    return;
	}
	return;
}

main {
	
	mut AA := [1, 3, 5, 7, 11];
	print AA $n;
	print apply(AA) $n;	
	print  $n;
	
	print "all done." $n;
}
