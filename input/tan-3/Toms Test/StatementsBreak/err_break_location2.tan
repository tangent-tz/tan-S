subr int syrup(int number) {
	var habanero := 0;
	if((number / 2) * 2 == number) {
		habanero := number / 2;
	} 
	else {
		habanero := 3 * number + 1;
	}
	break;
	return habanero;
}

main {
	const min := 10;
	const max := 100;
	
	var count := min;
	while (count < max) {
		call syrup(count);
		count := count + 1;
	}
	print "done." \n;
}
