main {
    const floatone := 10.5;
    const floattwo := 3.25;

    # Basic subtraction
    const resultone := floatone - floattwo;
    print resultone;  # Output: 7.25
    print \n;

    # Subtraction with negative numbers
    const floatthree := -5.5;
    const floatfour := -2.75;
    const resulttwo := floatthree - floatfour;
    print resulttwo;  # Output: -2.75
    print \n;

    # Subtraction with mixed positive and negative numbers
    const floatfive := 8.75;
    const floatsix := -3.5;
    const resultthree := floatfive - floatsix;
    print resultthree;  # Output: 12.25
    print \n;

    # Subtraction resulting in zero
    const floatseven := 10.0;
    const floateight := 10.0;
    const resultfour := floatseven - floateight;
    print resultfour;  # Output: 0.0
    print \n;

    # Subtraction with large numbers
    const floatnine := 999.99;
    const floatten := 500.0;
    const resultfive := floatnine - floatten;
    print resultfive;  # Output: 499.99
    print \n;
}
