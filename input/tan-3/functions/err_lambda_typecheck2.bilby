
func [float] apply([int] a){
	imm result := alloc [float](length a);
	
	mut i := 0;
	while (i < length a) {
		result[i] := pax(a[i]);
		i := i + 1;
	}
	return result;
}

func int pax(int b) {
	if (b < 4) {
	    return b;
	}
	return b - b / 3 ;
}

main {
