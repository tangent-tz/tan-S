subr [float] apply ([int] a, <int> r){
	const result := alloc [float](length a);
	
	var i := 0;
	while (i < length a) {
		result[i] := pax(a[i]);
		i := i + 1;
	}
	return result;
}

subr float pax(int b) {
	if (b < 4) {
	    return 3.14;
	}
	return b / 3.0 ;
}

