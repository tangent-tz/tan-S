
subr [float] apply([int] a){
	const result := new [float](length a);
	
	var i := 0;
	while (i < length a) {
		[result:i] := pax([a:i]);
		i := i + 1;
	}
	return result;
}

subr int pax(int b) {
	if (b < 4) {
	    return b;
	}
	return b - b / 3 ;
}

main {
	const max := 5;
	
	if(max > 3) {
		print pax \n;
	}
	
	var AA := [1, 3, 5, 7, 11];
	print AA \n;
	call apply(AA);	
	var count := 0;
	print pax(1234) \n;
	
	print "all done." \n;
}
