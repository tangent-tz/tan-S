
subr [float] apply([int] a) {
	const result := new [float](length a);
	
	var i := 0;
	while (i < length a) {
		[result:i] := pax([a:i], false);
		i := i + 1;
	}
	return result;
}

subr float pax(int b, bool c) {
	if (c) {
	    return 3.14;
	}
	return b / 3.0 ;
}

