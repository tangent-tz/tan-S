func <int> holler(int number) {
	print number $s isOdd(number) $s;
	return <number .. number * number / 2 + 1>;
}

func bool isOdd(int number) {
	return number != (number / 2) * 2;
}

main {
	imm max := 6;
	
	mut count := 0;
	while (count <= max) {
		mut result := holler(count);
		print result $n;
		count := count + 1;
	}
	
	print max*max $s isOdd(max*max) $n;
	
	print "all done." $n;
}
