main {
    # Test 1: multiplying two positive floating-point numbers
    var floatone := 3.14;
    var floattwo := 2.5;
    var resultone := floatone * floattwo;
    print resultone;
    print \n;

    # Test 2: multiplying a positive floating-point number with a negative floating-point number
    floatone := 3.14;
    floattwo := -2.5;
    var resulttwo := floatone * floattwo;
    print resulttwo;
    print \n;

    # Test 3: multiplying a negative floating-point number with two negative floating-point number
    floatone := -3.14;
    floattwo := -2.5;
    var resultthree := floatone * floattwo;
    print resultthree;
    print \n;

    # Test 4: multiplying a floating-point number by zero
    floatone := 3.14;
    floattwo := 0.0;
    var resultfour := floatone * floattwo;
    print resultfour;
    print \n;

    # Test 5: multiplying floating-point numbers with different decimal precision
    floatone := 3.14159;
    floattwo := 2.71828;
    var resultfive := floatone * floattwo;
    print resultfive;
    print \n;

    # Test 6: test the associativity of the multiplication operation
    floatone := 2.0;
    floattwo := 3.0;
    var floatthree := 4.0;
    var resultsix := floatone * floattwo * floatthree;
    print resultsix;
    print \n;

    # Test 7: test for any possible edge cases or corner cases
    # edge case: multiplying zero by any floating-point number
    floatone := 0.0;
    floattwo := 3.14;
    var resultseven := floatone * floattwo;
    print resultseven;
    print \n;
}
