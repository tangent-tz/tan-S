	print cow(9, 10, 11, 12) $n;
	print cow(4, 5, cow(9, 10, 11, 12)[2], 7) $n;
	
	mut AA := cow(1, 2, [goat(21, 10, 3) as int], cow(4, 5, cow(9, 10, 11, 12)[2], 7)[3]);
