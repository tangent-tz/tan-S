main {
    var a := 12.3;
    var b := 45;
    print "a: " \ a \n;
    print "b: " \ b \n;
    {
        print "<Enter inner scope1" \n;

        var a := 'J';
        print "a: " \ a \n;
        print "b: " \ b \n;

        {
            print "<Enter inner scope2" \n;
            print "a: " \ a \n;
            print "b: " \ b \n;

            b := 10234;
            print "a: " \ a \n;
            print "b: " \ b \n;

            print "Exit inner scope2>" \n;
        }

        print "a: " \ a \n;
        print "b: " \ b \n;

        print "Exit inner scope1>" \n;
    }
    print "a: " \ a \n;
    print "b: " \ b \n;
}