
func [float] apply([int] a) {
	imm result := alloc [float](length a);
	
	mut i := 0;
	while (i < length a) {
		result[i] := pax(a[i], false);
		i := i + 1;
	}
	return result;
}

func float pax(int b, bool c) {
	if (c) {
	    return 3.14;
	}
	return b / 3.0 ;
}

