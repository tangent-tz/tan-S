
func [int] cow(int a, int b, int c, int d){
	imm e := a;
	imm f := a + b;
	imm g := a + b + c;
	imm h := a + b + c + d;
	imm result := [e, f, g, h];
	return result;
}

func float goat(int w, int x, int y) {
	imm a := [w as float] / x;
	imm b :=  [w as float] / y;
	
	if (w > 24) {
	    return a;
	}
	return b;
}

