
subr [int] cow(int a, int b, int c, int d){
	const e := a;
	const f := a + b;
	const g := a + b + c;
	const h := a + b + c + d;
	const result := [e, f, g, h];
	return result;
}

subr float goat(int w, int x, int y) {
	const a := <float>(w) / x;
	const b := <float>(w) / y;
	
	if (w < 24) {
	    return a;
	}
	return b;
}

