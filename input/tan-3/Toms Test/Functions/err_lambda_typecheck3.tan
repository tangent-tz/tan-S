subr [float] apply([int] a){
	const result := new [float](length a);
	
	var i := 0;
	while (i < length a) {
		[result:i] := pax([a:i]);
		i := i + 1;
	}
	return result;
}

subr void pax(int b){
	if (b < 4) {
	    return;
	}
	return;
}

main {
	
	var AA := [1, 3, 5, 7, 11];
	print AA \n;
	print apply(AA) \n;
	print  \n;
	
	print "all done." \n;
}
